{
  "name": "bunny-poop",
  "version": "1.0.0",
  "description": "A social/idle web browser game",
  "main": "src/main.js",
  "dependencies": {
    "express": "^4.15.2",
    "http-server": "^0.9.0",
    "pixi.js": "^4.4.1"
  },
  "devDependencies": {
    "eslint": "^3.18.0",
    "eslint-config-standard": "^7.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.1.1",
    "jsdoc": "^3.4.3"
  },
  "scripts": {
    "eslint": "eslint src && eslint public/js",
    "pretest": "npm install",
    "test": "npm run eslint",
    "preversion": "npm run test",
    "version": "",
    "postversion": "git push && git push --tags && npm publish",
    "start": "node src/main.js",
    "deploy-minor": "npm version minor -m \"version %s\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crainone/bunny-poop.git"
  },
  "eslintConfig": {
    "env": {
	  "node": true
    },
    "extends": "node-style-guide",
    "rules": {
      "space-after-keywords": "off",
      "keyword-spacing": [2, {"before": true, "after": true}]
    }
  },
  "author": "crainone",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/crainone/bunny-poop/issues"
  },
  "homepage": "https://github.com/crainone/bunny-poop#readme"
}
